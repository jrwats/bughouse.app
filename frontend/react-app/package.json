    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.4",
    "@mui/styles": "^5.10.3",
    "@reach/router": "^1.3.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@testing-library/user-event": "^14.4.3",
    "chessground": "^8.3.5",
    "firebase": "^9.9.4",
    "invariant": "^2.2.4",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-error-boundary": "^3.1.4",
    "react-firebaseui": "^6.0.0",
    "react-relay": "^14.1.0",
    "react-scripts": "5.0.1",
    "react-scripts": "5.0.1",
    "relay-runtime": "^14.1.0",
    "web-vitals": "^2.1.4"
  },
  "engines": {
    "node": "16.x.x"
  },
  "scripts": {
    "relay": "relay-compiler --src ./src --schema ./data/schema.graphql",
    "start": "DEV=1 PORT=5000 react-scripts start",
    "start_https": "PORT=5000 HTTPS=true SSL_CRT_FILE=localhost.crt SSL_KEY_FILE=localhost.key react-scripts start",
    "preinstall": "./ensure-env.sh || echo 'Could not get secrets'",
    "build": "react-scripts build",
    "build:dev": "REACT_APP_FIREBASE=DEV REACT_APP_SOCKET_HOST=https://localhost:7777 react-scripts build",
    "start:dev": "PORT=5000 serve --cors --ssl-cert localhost.crt --ssl-key localhost.key -s build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-relay": "^14.1.0",
    "graphql": "^16.6.0",
    "relay-compiler": "^14.1.0",
    "relay-config": "^12.0.0",
    "serve": "^14.0.1"
  }
